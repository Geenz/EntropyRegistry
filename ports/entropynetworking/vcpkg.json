{
  "name": "entropynetworking",
  "version": "0.2.5",
  "port-version": 0,
  "description": "Networking library for Entropy Engine with Cap'n Proto and WebRTC support",
  "homepage": "https://github.com/Geenz/EntropyNetworking",
  "license": "MPL-2.0",
  "supports": "!uwp",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    "entropycore",
    "capnproto",
    "libdatachannel",
    "openssl",
    "zstd",
    "gtest",
    "tinyxml2",
    "llhttp",
    "cpp-httplib",
    {
      "name": "curl",
      "features": [
        "http2",
        "ssl"
      ]
    }
  ]
}
